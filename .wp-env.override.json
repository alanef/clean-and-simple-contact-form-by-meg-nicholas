{
  "plugins": [
    "./clean-and-simple-contact-form-by-meg-nicholas"
  ],
  "port": 8500,
  "testsPort": 8501,
  "config": {
    "WP_DEBUG_LOG": "/var/www/html/wp-content/debug.log",
    "FWAS_DEBUG": true,
    "WP_FS__DEV_MODE": true,
    "WP_DISABLE_FATAL_ERROR_HANDLER": true,
    "JETPACK_DEV_DEBUG": true,
    "CSCF_USE_SMTP": true,
    "CSCF_SMTP_HOST": "host.docker.internal",
    "CSCF_SMTP_PORT": 1510,
    "CSCF_SMTP_AUTH": false,
    "CSCF_SMTP_SECURE": "",
    "CSCF_SMTP_DEBUG": false
  },
  "env": {
    "development": {
      "mysqlPort": 62500
    },
    "tests": {
      "mysqlPort": 62501
    }
  }
}